<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="user">

  <select id="selectAll" parameterType="test.pojo.User" resultType="test.pojo.User" >
    select * from tb_user where username=#{username} and age=#{age}
  </select>

</mapper>
